#include <stdio.h>
int main()
{
    int a, b, c;
    scanf("%d %d %d", &a, &b, &c);
 
    // sort the numbers in ascending order
    int min = a;
    if (b < min)
    {
        min = b;
    }
    if (c < min)
    {
        min = c;
    }
    int max = a;
    if (b > max)
    {
        max = b;
    }
    if (c > max)
    {
        max = c;
    }
    int mid = a + b + c - min - max;
    printf("%d\n%d\n%d\n\n", min, mid, max);
    printf("%d\n%d\n%d\n", a, b, c);
}
